{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.equip.demo.adminequipmentapp",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.17.4",
      "toolsId": "0229bdcb-8278-4871-a6c2-2017ddba34e0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/admin/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.equip.demo.adminequipmentapp.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "EquipmentsList",
          "target": "EquipmentsList"
        },
        {
          "name": "EquipmentsObjectPage",
          "pattern": "Equipments({EquipmentsKey}):?query:",
          "target": "EquipmentsObjectPage"
        }
      ],
      "targets": {
        "EquipmentsList": {
          "type": "Component",
          "id": "EquipmentsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Equipments",
              "variantManagement": "Page",
              "navigation": {
                "Equipments": {
                  "detail": {
                    "route": "EquipmentsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "GridTable"
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "EquipmentsObjectPage": {
          "type": "Component",
          "id": "EquipmentsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "navigation": {},
              "contextPath": "/Equipments",
              "sectionLayout": "Tabs",
              "controlConfiguration": {
                "tasks/@com.sap.vocabularies.UI.v1.LineItem#i18nEquipmentTaskDetails": {
                  "tableSettings": {
                    "type": "GridTable"
                  },
                  "columns": {
                    "DataField::ID": {
                      "width": "12rem"
                    },
                    "DataField::description": {
                      "width": "15rem"
                    },
                    "DataField::dueDate": {
                      "width": "10rem"
                    },
                    "DataField::priority_code": {
                      "width": "12rem"
                    },
                    "DataField::status_code": {
                      "width": "12rem"
                    },
                    "DataField::assignedTo_ID": {
                      "width": "14rem"
                    }
                  }
                },
                "issues/@com.sap.vocabularies.UI.v1.LineItem#i18nIssueDetails": {
                  "tableSettings": {
                    "type": "GridTable"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
